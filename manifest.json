{
  "name": "YouTube Data Collector",
  "version": "1.0",
  "description": "YouTube video data collector Extension!",
  "permissions": [
    "debugger",
    "activeTab",
    "declarativeContent",
    "storage",
    "https://www.google.com/",
    "http://www.google.com/",
    "https://www.youtube.com/",
    "http://gdata.youtube.com/"
  ],
  "browser_action": {
    "default_title": "YouTube Video Fetch",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "icons": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": [
        "jquery.min.js"
      ],
      "css": [],
      "matches": [
        "http://*.google.com/*",
        "https://*.google.com/*",
        "https://www.youtube.com/*",
        "http://gdata.youtube.com/*"
      ]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://gdata.youtube.com/; object-src 'self'",
  "manifest_version": 2
}